{
    "version": 2,
    "builds": [
        {
            "src": "my-app/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(?!assets|favicon\\.ico|manifest\\.json).*",
            "dest": "/my-app/index.html"
        }
    ]
}